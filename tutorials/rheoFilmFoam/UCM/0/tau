FoamFile
{
    format      ascii;
    class       volSymmTensorField;
    object      tau;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{

    freeSurface0
    {
        type            zeroGradient;
    }

    outlet
    {   
        type            zeroGradient;
    }

    inlet
    {
        type            zeroGradient;
    }
 
    frontAndBack
    {
        type empty;
    }
    
    symm
    {
        type symmetryPlane;
    }    
}

