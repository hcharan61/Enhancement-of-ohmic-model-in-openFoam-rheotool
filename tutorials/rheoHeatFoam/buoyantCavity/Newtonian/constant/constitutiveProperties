FoamFile
{
    format      ascii;
    class       dictionary;
    object      constitutiveProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

parameters
{                
                type             Newtonian;

                rho              rho [1 -3 0 0 0 0 0] 1.;
                eta              eta [1 -1 -1 0 0 0 0] 0.71;
                
                 
                thermoEta
                {
                  type          Constant;
                }
                         
}

passiveScalarProperties
{
                solvePassiveScalar   no;
                D                    D [ 0 2 -1 0 0 0 0 ] 1e-9; 
}

