FoamFile
{
    format      ascii;
    class       volSymmTensorField;
    object      theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform ( 0 0 0 0 0 0);
 

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0 0 0 0 );
    }
    "(wall.*)"
    {
        type            zeroGradient;
    }
   
    outlet
    {
        type            zeroGradient;
    }
     
    frontAndBack
    {
        type             empty;
    }
}

